+            textStyle && textStyle[position],
+            customTextStyle,
+        ]} parse={[
+            ...parsePatterns(linkStyle),
+            { type: 'url', style: linkStyle, onPress: onUrlPress },
+            { type: 'phone', style: linkStyle, onPress: onPhonePress },
+            { type: 'email', style: linkStyle, onPress: onEmailPress },
