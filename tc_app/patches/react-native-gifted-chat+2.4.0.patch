+                case 0:
+                    Linking.openURL(`tel:${phone}`).catch(e => {
+                        error(e, 'No handler for telephone');
+                    });
+                    break;
+                case 1:
+                    Linking.openURL(`sms:${phone}`).catch(e => {
+                        error(e, 'No handler for text');
+                    });
+                    break;
+                default:
+                    break;
+            }
+        });
+    };
